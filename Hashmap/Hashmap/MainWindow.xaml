<Window x:Class="Hashmap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hashmap"
        mc:Ignorable="d"
        Title="Hashmap" Height="174" Width="382">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="46,28,0,0" Text="Key" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Margin="70,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="270"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Margin="70,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="270"/>
        <Label Content="Value" HorizontalAlignment="Left" Margin="26,54,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Margin="63,104,0,0" VerticalAlignment="Top" Width="83" Click="btnAdd_Click"/>
        <Button x:Name="btnRemove" Content="Remove" HorizontalAlignment="Left" Margin="160,104,0,0" VerticalAlignment="Top" Width="90" Click="btnRemove_Click"/>
        <Button x:Name="btnShowall" Content="Show All" HorizontalAlignment="Left" Margin="263,104,0,0" VerticalAlignment="Top" Width="77" Click="btnShowall_Click"/>

    </Grid>
</Window>
